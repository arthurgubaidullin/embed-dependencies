{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "cli": "nx",
  "title": "EmbedDependencies executor",
  "description": "",
  "type": "object",
  "properties": {
    "outputPath": {
      "type": "string"
    },
    "sourceDist": {
      "type": "string"
    },
    "injector": {
      "type": "string",
      "anyOf": [{ "const": "yalc" }, { "const": "npm-pack" }],
      "default": "yalc"
    }
  },
  "required": ["outputPath", "sourceDist"]
}
